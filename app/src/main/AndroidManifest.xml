<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.centerm.sinopecsdktest">

    <uses-feature android:name="android.hardware.mCamera" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="posapp.com.qgsypos.sdk.permission.SCANNER" />
    <uses-permission android:name="posapp.com.qgsypos.sdk.permission.PRINTER" />
    <uses-permission android:name="posapp.com.qgsypos.sdk.permission.DEVICE_INFO" />
    <uses-permission android:name="posapp.com.qgsypos.sdk.permission.DEVICE_MANAGER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PreviewScannActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ManagerActivity" />
        <activity android:name=".PrinterActivity" />
        <activity android:name=".ScannerActivity" />
        <activity android:name=".SystemActivity" />
    </application>

</manifest>